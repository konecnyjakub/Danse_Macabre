#. [unit]: id=Merrin, type=Soulless Hero
#. [part]
#. [variation]
#: Danse_Macabre/scenarios/01_beginning_of_the_tragedy.cfg:765
#: Danse_Macabre/scenarios/15_dawn_of_the_dead.cfg:34
#: Danse_Macabre/units/heroes/Battle_Heroes.cfg:156
#: Danse_Macabre/units/heroes/Brainless_Heroes.cfg:156
#: Danse_Macabre/units/heroes/Running_Heroes.cfg:153
