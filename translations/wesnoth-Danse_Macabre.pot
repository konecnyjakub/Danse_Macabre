#. [side]: type=Armageddon Drake, id=drakeboss
#: Danse_Macabre/scenarios/13_day_of_the_dead.cfg:69
msgid "Smaug"
msgstr ""

#. [side]: type=Dwarvish Runemaster, id=dwarfboss
#: Danse_Macabre/scenarios/13_day_of_the_dead.cfg:91
msgid "Gloin"
msgstr ""

#. [side]: type=Great Troll, id=trollboss
#: Danse_Macabre/scenarios/13_day_of_the_dead.cfg:112
msgid "Troll"
msgstr ""

#. [side]: type=Great Troll, id=trollboss
#: Danse_Macabre/scenarios/13_day_of_the_dead.cfg:115
msgid "Skalmad"
msgstr ""

#. [side]: type=Elvish Sylph, id=elfboss
#: Danse_Macabre/scenarios/13_day_of_the_dead.cfg:137
msgid "Galadriel"
msgstr ""

#. [objective]: condition=win
#: Danse_Macabre/scenarios/13_day_of_the_dead.cfg:169
msgid "Defeat Enemy Leaders"
