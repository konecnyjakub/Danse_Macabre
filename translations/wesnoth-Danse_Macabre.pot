#. [unit]: type=Death Knight, id=bakabone
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:183
msgid "Vagabone"
msgstr ""

#. [message]: speaker=Macabre
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:247
msgid "This is the den of the lich. We must exterminate him, but be careful! We don't know what traps he set up..."
msgstr ""

#. [message]: speaker=narrator
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:257
msgid "IT'S TOO LATE!!"
msgstr ""

#. [message]: speaker=Macabre
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:273
msgid "!!"
msgstr ""

#. [message]: speaker=Mal Oryzias
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:277
msgid "You have no chance to get away. We've got you here!"
msgstr ""

#. [message]: speaker=Macabre
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:281
msgid "Bah! I'm not about to get away! Just you wait!!"
msgstr ""

#. [message]: speaker=narrator
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:314
msgid "Never wake him up! He may completely ignore any puppets, though. Oryzias"
msgstr ""

#. [message]: speaker=unit
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:340
msgid "What is this space?"
msgstr ""

#. [unit]: type=Vampire, id=orlok
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:356
msgid "Orlok"
msgstr ""

#. [message]: speaker=orlok
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:376
msgid "Who are you?"
msgstr ""

#. [message]: speaker=unit
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:387
msgid "I am Macabre."
msgstr ""

#. [message]: speaker=orlok
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:391
msgid "Macabre? I know that name...but aren't you a zombie?"
msgstr ""

#. [message]: speaker=unit
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:395
msgid "I was born again."
msgstr ""

#. [message]: speaker=unit
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:401
msgid "I...I'm a follower of Mr. Macabre."
msgstr ""

#. [message]: speaker=orlok
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:405
msgid "Macabre? Ah... I have recently heard that there is an intelligent zombie that goes by that name. Is he here?"
msgstr ""

#. [message]: speaker=unit
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:409
msgid "Now he is not a zombie...but you are right about the rest."
msgstr ""

#. [message]: speaker=orlok
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:416
msgid "Hm...and what are you doing now?"
msgstr ""

#. [message]: speaker=unit
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:420
msgid "We must kill an ancient lich."
msgstr ""

#. [message]: speaker=orlok
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:424
msgid "Ancient lich? Ho...sounds fun."
msgstr ""

#. [message]: speaker=orlok
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:428
msgid "I'm an elder vampire named Orlok. Shall I do a favor for you?"
msgstr ""

#. [message]: speaker=unit
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:432
msgid "Really? How comforting! Of course yes!"
msgstr ""

#. [message]: speaker=Macabre
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:477
msgid "Give us your heart, SON OF A LICH!!"
msgstr ""

#. [message]: speaker=crows
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:481
msgid "Noooooo!!"
msgstr ""

#. [message]: speaker=Mal Oryzias
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:485
msgid "I'm not gonna let you!!!"
msgstr ""

#. [message]: speaker=Mal Oryzias
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:510
msgid "Muhahahahahahahaha!!"
msgstr ""

#. [message]: speaker=Macabre
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:514
msgid "Damn it!!"
msgstr ""

#. [message]: speaker=Mal Oryzias
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:525
msgid "Heck!"
msgstr ""

#. [message]: speaker=Mal Oryzias
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:530
msgid "Vagabone! Close the rear!!"
msgstr ""

#. [message]: speaker=Macabre
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:538
msgid "Do you show your back to the enemy? Shame on you!!"
msgstr ""

#. [message]: speaker=Mal Oryzias
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:542
msgid "Humph!! I will tear through this cave!"
msgstr ""

#. [message]: speaker=Macabre
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:546
#: Danse_Macabre/scenarios/14_bridge.cfg:100
#: Danse_Macabre/scenarios/14_night_of_the_living_dead.cfg:1699
msgid "What!?"
msgstr ""

#. [message]: speaker=Mal Oryzias
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:550
msgid "Have fun!!"
msgstr ""

#. [message]: speaker=Macabre
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:554
#: Danse_Macabre/scenarios/15_dawn_of_the_dead.cfg:412
msgid "Wait!!"
msgstr ""

#. [message]: speaker=Macabre
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:572
msgid "God!!"
msgstr ""

#. [message]: speaker=Macabre
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:576
msgid "Hey you, Vagabone! Why do you obey that bastard!? He left you to die!!"
msgstr ""

#. [message]: speaker=bakabone
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:580
msgid "Meh"
msgstr ""

#. [message]: speaker=Macabre
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:584
msgid "...OK, you are the paragon of the undead!! Tear him apart!! Hurry up! We must break away and chase after the lich!!"
msgstr ""

#. [objective]: condition=win
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:593
msgid "Macabre must reach the signpost."
msgstr ""

#. [message]: speaker=Macabre
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:634
msgid "All right! Let's chase after the lich!"
msgstr ""

#. [scenario]: id=13_day_of_the_dead
#: Danse_Macabre/scenarios/13_day_of_the_dead.cfg:6
msgid "Day of the dead"
msgstr ""

#. [side]: type=Grand Marshal, id=humanboss
#: Danse_Macabre/scenarios/13_day_of_the_dead.cfg:44
msgid "Theoden"
